    <style>
        @@media only screen and (max-width: 991px) {
            .main-header-top .top-nav > li:nth-child(2), .main-header-top .top-nav > li:nth-child(4), .main-header-top .top-nav > li:nth-child(3), .main-header-top .top-nav > li:nth-child(5) span:nth-child(2) {
                display: none;
            }
        }

        label {
            color: rgba(43, 61, 81, 0.8);
            text-transform: initial;
        }

        .btn {
            text-transform: initial;
            border-radius: 5px;
            -webkit-box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
            box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
            font-size: 13px;
            font-weight: 600;
            -webkit-transition: box-shadow linear 0.4s;
            -moz-transition: box-shadow linear 0.4s;
            -ms-transition: box-shadow linear 0.4s;
            transition: box-shadow linear 0.4s;
        }

        .card {
            margin-bottom: 30px;
            border: none;
            box-shadow: 0px 0px 7px rgb(0 0 0 / 13%);
        }

        .card-header-text {
            margin-bottom: 0;
            font-size: 1rem;
            text-transform: initial;
            font-weight: bold;
            display: inline-block;
            vertical-align: middle;
            color: rgb(99 98 98);
        }

        html, body {
            margin: 0;
            padding: 0;
            height: 100%;
        }

        body {
            background-image: url('/assets/images/Fin.jpg');
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
        }

        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: #333;
            color: white;
            text-align: center;
            padding: 20px;
        }

        .footer a {
            color: white;
            text-decoration: none;
            margin: 0 10px;
        }

        .footer a:hover {
            text-decoration: underline;
        }
    </style>

</head>
